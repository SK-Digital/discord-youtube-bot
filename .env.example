# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
PREFIX=!
MAX_FILE_SIZE_MB=10

# Test Guild ID (for instant command registration)
TEST_GUILD_ID=your_test_guild_id_here

# Logging Configuration
LOG_LEVEL=INFO

# SSL Configuration (for macOS/Linux)
SSL_CERT_FILE=/path/to/certifi/ca-bundle.crt
REQUESTS_CA_BUNDLE=/path/to/certifi/ca-bundle.crt

# YouTube Cookies (optional - for age-restricted or region-locked content)
# Paste the entire contents of your cookies.txt file here as a single line
# Make sure to escape any special characters if needed
YOUTUBE_COOKIES=
